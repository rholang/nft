{
    "name": "@rholang/connectors",
    "description": "File system Router for Vite & React",
    "version": "1.1.3",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "author": "morelearn1990 <morelearn1990@gmail.com>",
    "homepage": "https://github.com/morelearn1990/vite-plugin-react-router",
    "bugs": "https://github.com/morelearn1990/vite-plugin-react-router/issues",
    "files": [
        "dist"
    ],
    "tsup": {
        "splitting": false,
        "sourcemap": true,
        "clean": true,
        "entryPoints": [
            "src/index.ts"
        ]
    },
    "scripts": {
        "dev": "pnpm run build -- --watch --ignore-watch examples",
        "build": "vite build",
        "prepublishOnly": "pnpm run build",
        "release": "pnpx git-ensure -a && pnpx bumpp --commit --tag --push",
        "example:dev": "pnpm -C examples/demo run dev",
        "example:build": "pnpm -C examples/demo run build",
        "example:preview": "pnpm -C examples/demo run preview",
        "test": "jest",
        "lint": "eslint --fix --ext .ts ./",
        "commitprepare": "pnpx husky add .husky/commit-msg 'pnpx commitlint --edit $1'",
        "lintprepare": "pnpx husky add .husky/pre-commit 'pnpx lint-staged'",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
        "preinstall": "npx only-allow pnpm"
    },
    "devDependencies": {
        "buffer": "^6.0.3",
        "axios": "^0.21.1",
        "@types/node": "^16.4.13",
        "@types/ethereumjs-util": "^6.1.0",
        "ethereumjs-util": "^7.0.7",
        "tsup": "^4.11.2",
        "@babel/plugin-syntax-jsx": "^7.14.5",
        "elliptic": "^6.5.3",
        "@types/google-protobuf": "^3.7.4",
        "google-protobuf": "^3.14.0",
        "@types/elliptic": "^6.4.12",
        "dayjs": "^1.10.4",
        "js-sha3": "^0.8.0",
        "@types/bs58": "^4.0.1",
        "@types/cron": "^1.7.2",
        "blakejs": "^1.1.0",
        "bs58": "^4.0.1",
        "@types/ramda": "^0.27.40",
        "ramda": "^0.27.1",
        "@effector/reflect": "latest",
        "effector": "latest",
        "effector-logger": "^0.9.0",
        "effector-react": "latest",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-typescript": "^7.14.5",
        "@types/jest": "^26.0.24",
        "faker": "^5.4.0",
        "jest-localstorage-mock": "^2.4.8",
        "ts-babel": "^6.1.7",
        "typescript": "^4.3.5",
        "vite": "^2.3.8",
        "vite-plugin-dts": "^0.5.2",
        "@rholang/vite-plugin-rholang": "workspace:*",
        "@tgrospic/rnode-http-js": "^0.1.6",
        "rollup-plugin-typescript2": "0.30.0"
    }
}
